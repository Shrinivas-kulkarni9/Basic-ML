    "#Now, this neural net is to illustrate vanishing gradient\n",
    "#CHose all activations as sigmoids\n",
    "\n",
    "D_in, H1, H2, H3, H4, D_out = inputsize, 10, 5, 5, 5, 1 #You can add more layers if you wish to \n",
    "\n",
    "neurons = [D_in, H1, H2, H3, H4, D_out] # list of number of neurons in the layers sequentially.\n",
    "activation_functions = ['linear','sigmoid','sigmoid','sigmoid', 'sigmoid'] #activations in each layer (Note: the input layer does not have any activation)\n",
    "my_neuralnet_Vanishing = Neural_Network(neurons, activation_functions )\n",
    "#\n",
    "minibatch = False"
   ]
