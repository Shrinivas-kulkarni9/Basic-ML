    "X = []\n",
    "Y = []\n",
    "inputsize = 3\n",
    "minibatch = False\n",
    "onehotencoded = False\n",
    "n_batch = 20\n",
    "batch_size = 5\n",
    "for i in range(500):\n",
    "    if(i % 2 == 0):\n",
    "        X.append([random.randint(1,10) for i1 in range(inputsize)])\n",
    "        Y.append(1)\n",
    "    else:\n",
    "        X.append([random.randint(-10,1) for i1 in range(inputsize)])\n",
    "        Y.append(0)\n",
    "X = np.array(X)\n",
    "Y = np.array(Y)\n",
    "if onehotencoded:\n",
    "    Y = onehotencoding(Y)\n",
    "\n",
    "if minibatch==False:\n",
    "    train_X = X\n",
    "    train_Y = Y\n",
    "else:\n",
    "    pass\n",
    "# !!!!!! I HAVE REDONE THE CODE FOR MINIBATCH SETTING AFTERWARDS. !!!!!!"
